{
  "schemaVersion": 1,
  "id": "gwwhit",
  "version": "${version}",

  "name": "Guess What Will Happen In It",
  "description": "Fabric mod where anyone can PR anything, concerning or not. I'll merge everything as soon as it works.",
  "authors": [
    "anatom3000",
    "ENDERZOMBI102",
    "MattiDragon",
    "JFronny",
    "C10udburst",
    "LordDeatHunter",
    "SekoiaTree",
    "Ryorama",
    "Minenash",
    "slambo312",
    "Noaaan",
    "MalekiRe",
    "IMS212",
    "PheonixVX",
    "KosmX"
  ],
  "contact": {
    "sources": "https://github.com/anatom3000/GuessWhatWillHappenInThis"
  },

  "license": "MIT",
  "icon": "assets/gwwhit/icon.png",

  "environment": "*",
  "entrypoints": {
    "modmenu": [
      "fr.anatom3000.gwwhit.config.ModmenuIntegration"
    ],
    "main": [
      "fr.anatom3000.gwwhit.GWWHIT"
    ],
    "client": [
      "fr.anatom3000.gwwhit.GWWHITClient"
    ],
    "libjf:early": [
      "fr.anatom3000.gwwhit.util.ConsoleHacker",
      "fr.anatom3000.gwwhit.util.Screenshotter"
    ]
  },
  "mixins": [
    "gwwhit.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.12.12",
    "fabric": "*",
    "minecraft": "1.18.x",
    "cloth-config2": ">=6.1.48",
    "jythonmc": ">=1.0.2",
    "libjf-data-manipulation-v0": ">=2.4.0",
    "libjf-unsafe-v0": ">=2.4.0"
  },

  "suggests": {
    "modmenu": "*"
  },

  "custom": {
    "jythonmc:use": true,
    "codename": "${codename}"
  }
}
